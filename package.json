{
  "name": "yunzai-next",
  "version": "4.1.37",
  "author": "Le-niao",
  "description": "QQ Group Bot",
  "main": "./src/main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "app": "node src/main.js",
    "login": "node src/main.js --login",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "kill": "pm2 kill",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "pm2": "pm2",
    "build": "rollup --config rollup.config.js",
    "version-app": "npx tsx src/version.ts",
    "ts-app": "npx tsx src/main.js",
    "test-app": "npx tsx src/main.ts",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "icqq": "^0.6.10",
    "image-size": "^1.1.1",
    "lodash": "^4.17.21",
    "oicq": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-puppeteer": "^1.0.3",
    "yunzai": "link:yunzai",
    "yunzai-mys": "^1.0.5",
    "yz-system": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/inquirer": "^9.0.7",
    "@types/lodash": "^4.17.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.8.5",
    "@types/node-localstorage": "^1.3.3",
    "@types/node-schedule": "^2.1.7",
    "@types/react": "^18.3.3",
    "@types/redis": "^4.0.11",
    "babel-plugin-module-resolver": "^5.0.2",
    "husky": "^9.1.1",
    "prettier": "^3.3.3",
    "rollup": "^4.16.4",
    "typescript": "^5.5.4"
  },
  "workspaces": [
    "plugins/**",
    "renderers/**"
  ],
  "imports": {
    "#yunzai": "./lib/index.js"
