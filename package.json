{
  "name": "yunzai",
  "version": "4.1.0-rc.0",
  "author": "Yoimiya-Kokomi, Le-niao",
  "description": "QQ Group Bot",
  "main": "./src/main.js",
  "type": "module",
  "scripts": {
    "app": "node src/main.js",
    "login": "node src/main.js login",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "kill": "pm2 kill",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "pm2": "pm2",
    "build": "rollup --config rollup.config.js",
    "css:watch": "tailwindcss -i ./src/input.css -o ./public/output.css --watch  -m",
    "css": "tailwindcss -i ./src/input.css -o ./public/output.css -m",
    "version": "node src/version.js",
    "lint": "commitlint --config .commitlint.config.cjs -e -V",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "yunzai": "link:yunzai",
    "yz-mw-runtime": "link:./middleware/runtime",
    "yz-mw-star-rail": "link:./middleware/star-rail",
    "yz-system": "^1.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/inquirer": "^9.0.7",
    "@types/koa": "^2.15.0",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/leveldown": "^4.0.6",
    "@types/levelup": "^5.1.5",
    "@types/lodash": "^4.14.200",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.8.5",
    "@types/node-localstorage": "^1.3.3",
    "@types/node-schedule": "^2.1.7",
    "@types/react-dom": "^18.2.22",
    "@types/redis": "^4.0.11",
    "@types/ws": "^8.5.7",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "preline": "^2.3.0",
    "prettier": "^3.0.3",
    "rollup": "^4.16.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
