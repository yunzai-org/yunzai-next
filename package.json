{
  "name": "yunzai-next",
  "version": "4.1.29",
  "author": "Yoimiya-Kokomi, Le-niao",
  "description": "QQ Group Bot",
  "main": "./src/main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "app": "node src/main.js",
    "login": "node src/main.js --login",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "kill": "pm2 kill",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "pm2": "pm2",
    "build": "rollup --config rollup.config.js",
    "version-app": "node src/version.js",
    "ts-app": "npx tsx src/main.js",
    "test-app": "npx tsx src/main.ts",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "react-puppeteer": "^1.0.3",
    "yunzai": "link:yunzai",
    "yunzai-mys": "link:yunzai-mys",
    "yz-mw-runtime": "link:middleware/runtime",
    "yz-system": "^1.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/lodash": "^4.14.200",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.8.5",
    "@types/node-localstorage": "^1.3.3",
    "@types/node-schedule": "^2.1.7",
    "@types/react-dom": "^18.2.22",
    "@types/redis": "^4.0.11",
    "@types/ws": "^8.5.7",
    "husky": "^9.1.1",
    "prettier": "^3.3.3",
    "rollup": "^4.16.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "workspaces": [
    "plugins/**",
    "middleware/**",
    "renderers/**"
  ],
  "imports": {
    "#yunzai": "./lib/index.js"
  }
}
