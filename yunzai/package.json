{
  "name": "yunzai",
  "version": "1.0.0-rc.0",
  "author": "Yoimiya-Kokomi, Le-niao, ningmengchongshui",
  "description": "QQ Group Bot",
  "main": "./index.js",
  "type": "module",
  "files": [
    "css.url.js",
    "index.js",
    "config/**/*.js",
    "core/**/*.js",
    "db/**/*.js",
    "image/**/*.js",
    "init/**/*.js",
    "mys/**/*.js",
    "tsconfig/**/*.js",
    "utils/**/*.js",
    ".puppeteerrc.cjs",
    "main.css",
    "yunzai-env.d.ts",
    "global.d.ts",
    "tsconfig/tsconfig.json"
  ],
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./config": {
      "import": "./config/index.js"
    },
    "./db": {
      "import": "./db/index.js"
    },
    "./mys": {
      "import": "./mys/index.js"
    },
    "./utils": {
      "import": "./utils/index.js"
    },
    "./core": {
      "import": "./core/index.js"
    },
    "./init": {
      "import": "./init/index.js"
    },
    "./image": {
      "import": "./image/index.js"
    },
    "./.puppeteerrc": {
      "require": "./.puppeteerrc.cjs"
    },
    "./env": {
      "types": "./yunzai-env.d.ts"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./tsconfig/tsconfig.json": {
      "import": "./tsconfig/tsconfig.json",
      "require": "./tsconfig/tsconfig.json"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
